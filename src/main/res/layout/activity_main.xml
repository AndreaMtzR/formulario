<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4A009688"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/etnombre"
        android:layout_width="257dp"
        android:layout_height="55dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        android:text="nombre"
        app:layout_constraintBottom_toTopOf="@+id/etApellido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.837"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42" />

    <EditText
        android:id="@+id/eTcolonia"
        android:layout_width="259dp"
        android:layout_height="54dp"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:hint="Colonia"
        android:inputType="textPersonName|phone"
        android:text="colonia"
        app:layout_constraintBottom_toTopOf="@+id/etcp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etcp"
        android:layout_width="260dp"
        android:layout_height="53dp"
        android:layout_marginBottom="48dp"
        android:ems="10"
        android:hint="cp"
        android:inputType="textWebEmailAddress|textPersonName"
        android:text="cp"
        app:layout_constraintBottom_toTopOf="@+id/etEstado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etEstado"
        android:layout_width="262dp"
        android:layout_height="52dp"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:hint="estado"
        android:inputType="textPersonName|numberPassword"
        android:text="estado"
        app:layout_constraintBottom_toTopOf="@+id/etMunicipio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.832"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etMunicipio"
        android:layout_width="263dp"
        android:layout_height="51dp"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:hint="municipio"
        android:inputType="textPersonName"
        android:text="municipio"
        app:layout_constraintBottom_toTopOf="@id/button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:onClick="registrar"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etMunicipio"
        app:layout_constraintVertical_bias="0.218" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="100dp"
        android:onClick="limpiar"
        android:text="Limpiar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/etApellido"
        android:layout_width="258dp"
        android:layout_height="57dp"
        android:layout_marginBottom="12dp"
        android:ems="10"
        android:hint="Apellido"
        android:inputType="textPersonName"
        android:text="apellido pat y mat"
        app:layout_constraintBottom_toTopOf="@+id/eTcolonia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="13dp"
        android:text="Escribe nombre"
        app:layout_constraintBottom_toBottomOf="@+id/etnombre"
        app:layout_constraintEnd_toStartOf="@+id/etnombre"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="12dp"
        android:text="Escribe apellidos"
        app:layout_constraintEnd_toStartOf="@+id/etApellido"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="13dp"
        android:text="escribe colonia"
        app:layout_constraintBottom_toBottomOf="@+id/eTcolonia"
        app:layout_constraintEnd_toStartOf="@+id/eTcolonia"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="11dp"
        android:text="ingresa CP"
        app:layout_constraintBottom_toBottomOf="@+id/etcp"
        app:layout_constraintEnd_toStartOf="@+id/etcp"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="78dp"
        android:text="ingresa estado"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/etEstado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="12dp"
        android:text="ingresa municipio"
        app:layout_constraintBottom_toBottomOf="@+id/etMunicipio"
        app:layout_constraintEnd_toStartOf="@+id/etMunicipio"
        app:layout_constraintHorizontal_bias="0.339"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>